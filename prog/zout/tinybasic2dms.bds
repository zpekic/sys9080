021f 021f d 220f08
021f 021f s         SHLD LOPLN
0222 0222 d eb
0222 0222 s         XCHG                            ;AND TEXT POINTER
0223 0223 d 221108
0223 0223 s         SHLD LOPPT
0226 0226 d 010a00
