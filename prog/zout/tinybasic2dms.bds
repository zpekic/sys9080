0229 0229 d 2a0908
0229 0229 s         LHLD LOPVAR                     ;FIND 'LOPVAR'
022c 022c d eb
022c 022c s         XCHG
022d 022d d 60
022d 022d s         MOV  H,B
022e 022e d 68
022e 022e s         MOV  L,B                        ;HL=0 NOW
022f 022f d 39
022f 022f s         DAD  SP                         ;HERE IS THE STACK
0230 0230 d 3e
0230 0230 s         DB   3EH
0231 0231 d 09
0231 0231 s FR7:    DAD  B                          ;EACH LEVEL IS 10 DEEP
0232 0232 d 7e
0232 0232 s         MOV  A,M                        ;GET THAT OLD 'LOPVAR'
0233 0233 d 23
0233 0233 s         INX  H
0234 0234 d b6
0234 0234 s         ORA  M
0235 0235 d ca5202
0235 0235 s         JZ   FR8                        ;0 SAYS NO MORE IN IT
0238 0238 d 7e
0238 0238 s         MOV  A,M
0239 0239 d 2b
0239 0239 s         DCX  H
023a 023a d ba
023a 023a s         CMP  D                          ;SAME AS THIS ONE?
023b 023b d c23102
023b 023b s         JNZ  FR7
023e 023e d 7e
023e 023e s         MOV  A,M                        ;THE OTHER HALF?
023f 023f d bb
023f 023f s         CMP  E
0240 0240 d c23102
0240 0240 s         JNZ  FR7
0243 0243 d eb
0243 0243 s         XCHG                            ;YES, FOUND ONE
0244 0244 d 210000
0244 0244 s         LXI  H,0H
0247 0247 d 39
0247 0247 s         DAD  SP                         ;TRY TO MOVE SP
0248 0248 d 44
0248 0248 s         MOV  B,H
0249 0249 d 4d
0249 0249 s         MOV  C,L
024a 024a d 210a00
024a 024a s         LXI  H,0AH
024d 024d d 19
024d 024d s         DAD  D
024e 024e d cdee05
024e 024e s         CALL MVDOWN                     ;AND PURGE 10 WORDS
0251 0251 d f9
0251 0251 s         SPHL                            ;IN THE STACK
0252 0252 d 2a1108
0252 0252 s FR8:    LHLD LOPPT                      ;JOB DONE, RESTORE DE
0255 0255 d eb
